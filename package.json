{
  "name": "serverless-boilerplate-express-typescript",
  "version": "1.2.1",
  "description": "Serverless framework ExpressJS TypeScript",
  "scripts": {
    "clean": "rimraf dist .esbuild .serverless",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "commit": "cz",
    "test:dev": "start-server-and-test dev http://localhost:4000 'curl --head http://localhost:4000'",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cookies-parser": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.0",
    "morgan": "^1.10.0",
    "pino": "^8.15.6",
    "tsx": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/cross-spawn": "^6.0.3",
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "esbuild": "~0.16.17",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "mongodb-memory-server": "^8.16.0",
    "npm-run-all": "^4.1.5",
    "p-retry": "^4.6.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^19.0.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "author": "Shabir Khan"
}
